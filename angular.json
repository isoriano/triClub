{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "defaultProject": "portal-shell",
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "projects": {
    "portal-shell": {
      "projectType": "application",
      "root": "apps/portal-shell",
      "sourceRoot": "apps/portal-shell/src",
      "prefix": "tcs",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "outputPath": "dist",
            "index": "apps/portal-shell/src/index.html",
            "main": "apps/portal-shell/src/main.ts",
            "polyfills": "apps/portal-shell/src/polyfills.ts",
            "tsConfig": "apps/portal-shell/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/portal-shell/src/favicon.ico",
              "apps/portal-shell/src/assets/images",
              "apps/portal-shell/src/assets/i18n"
            ],
            "styles": ["apps/portal-shell/src/styles.scss"],
            "scripts": [],
            "extraWebpackConfig": "apps/portal-shell/webpack.config.js"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/portal-shell/src/environments/environment.ts",
                  "with": "apps/portal-shell/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ],
              "extraWebpackConfig": "apps/portal-shell/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": {
            "browserTarget": "portal-shell:build",
            "extraWebpackConfig": "apps/portal-shell/webpack.config.js",
            "port": 5000
          },
          "configurations": {
            "production": {
              "browserTarget": "portal-shell:build:production",
              "extraWebpackConfig": "apps/portal-shell/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "portal-shell:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "portal-shell:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/portal-shell/src/**/*.ts",
              "apps/portal-shell/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": ["coverage/apps/portal-shell"],
          "options": {
            "jestConfig": "apps/portal-shell/jest.config.js",
            "passWithNoTests": true,
            "extraWebpackConfig": "apps/portal-shell/webpack.config.js"
          }
        }
      }
    },
    "portal-shell-e2e": {
      "root": "apps/portal-shell-e2e",
      "sourceRoot": "apps/portal-shell-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/portal-shell-e2e/cypress.json",
            "tsConfig": "apps/portal-shell-e2e/tsconfig.e2e.json",
            "devServerTarget": "portal-shell:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "portal-shell:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/portal-shell-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
